<jcr:root xmlns:jcr="http://www.jcp.org/jcr/1.0"
          xmlns:cq="http://www.day.com/jcr/cq/1.0"
          xmlns:granite="http://www.adobe.com/jcr/granite/1.0"
          xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
          xmlns:sling="http://sling.apache.org/jcr/sling/1.0"
          jcr:primaryType="nt:unstructured"
          jcr:title="My Slider Component"
          sling:resourceType="cq/gui/components/authoring/dialog">
   <content jcr:primaryType="nt:unstructured"
            sling:resourceType="granite/ui/components/coral/foundation/container">
      <items jcr:primaryType="nt:unstructured">
         <tabs jcr:primaryType="nt:unstructured"
               sling:resourceType="granite/ui/components/coral/foundation/tabs">
            <items jcr:primaryType="nt:unstructured">
               <tab-1 jcr:primaryType="nt:unstructured"
                      jcr:title="Tab 1"
                      sling:resourceType="granite/ui/components/coral/foundation/container">
                  <layout jcr:primaryType="nt:unstructured"
                          sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"/>
                  <items jcr:primaryType="nt:unstructured">
                     <column jcr:primaryType="nt:unstructured"
                             sling:resourceType="granite/ui/components/coral/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                           <textareaTest cq-msm-lockable="./textareaTest"
                                         fieldLabel="Textarea Test"
                                         jcr:primaryType="nt:unstructured"
                                         name="./textareaTest"
                                         sling:resourceType="granite/ui/components/coral/foundation/form/textarea"/>
                           <textareatest1 cq-msm-lockable="./textareatest1"
                                          fieldLabel="Textarea Test1"
                                          jcr:primaryType="nt:unstructured"
                                          name="./textareatest1"
                                          sling:resourceType="granite/ui/components/coral/foundation/form/textarea"/>
                           <textareatest2 cq-msm-lockable="./textareatest2"
                                          fieldLabel="Textarea Test2"
                                          jcr:primaryType="nt:unstructured"
                                          name="./textareatest2"
                                          sling:resourceType="granite/ui/components/coral/foundation/form/textarea"/>
                           <textareatest3 cq-msm-lockable="./textareatest3"
                                          fieldLabel="Textarea Test3"
                                          jcr:primaryType="nt:unstructured"
                                          name="./textareatest3"
                                          sling:resourceType="granite/ui/components/coral/foundation/form/textarea"/>
                           <textareatest4 cq-msm-lockable="./textareatest4"
                                          fieldLabel="Textarea Test4"
                                          jcr:primaryType="nt:unstructured"
                                          name="./textareatest4"
                                          sling:resourceType="granite/ui/components/coral/foundation/form/textarea"/>
                           <textareatest5 cq-msm-lockable="./textareatest5"
                                          fieldLabel="Textarea Test5"
                                          jcr:primaryType="nt:unstructured"
                                          name="./textareatest5"
                                          sling:resourceType="granite/ui/components/coral/foundation/form/textarea"/>
                           <textareatest6 cq-msm-lockable="./textareatest6"
                                          fieldLabel="Textarea Test6"
                                          jcr:primaryType="nt:unstructured"
                                          name="./textareatest6"
                                          sling:resourceType="granite/ui/components/coral/foundation/form/textarea"/>
                        </items>
                     </column>
                  </items>
               </tab-1>
               <tab-2 jcr:primaryType="nt:unstructured"
                      jcr:title="Tab 2"
                      sling:resourceType="granite/ui/components/coral/foundation/container">
                  <layout jcr:primaryType="nt:unstructured"
                          sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"/>
                  <items jcr:primaryType="nt:unstructured">
                     <column jcr:primaryType="nt:unstructured"
                             sling:resourceType="granite/ui/components/coral/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                           <imageTestResType jcr:primaryType="nt:unstructured"
                                             name="./imageTest/sling:resourceType"
                                             sling:resourceType="granite/ui/components/coral/foundation/form/hidden"
                                             value="core/wcm/components/image/v2/image"/>
                           <imageTest allowUpload="{Boolean}false"
                                      autoStart="{Boolean}false"
                                      class="cq-droptarget"
                                      cq-msm-lockable="./imageTest/file"
                                      fieldLabel="Image Test"
                                      fileNameParameter="./imageTest/fileName"
                                      fileReferenceParameter="./imageTest/fileReference"
                                      jcr:primaryType="nt:unstructured"
                                      mimeTypes="[image/gif,image/jpeg,image/png,image/webp,image/tiff,image/svg+xml]"
                                      multiple="{Boolean}false"
                                      name="./imageTest/file"
                                      sling:resourceType="cq/gui/components/authoring/dialog/fileupload"
                                      title="Drag to select image"
                                      uploadUrl="${suffix.path}"
                                      useHTML5="{Boolean}true"/>
