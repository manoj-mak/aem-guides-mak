                           <imageTest3ResType jcr:primaryType="nt:unstructured"
                                              name="./imageTest3/sling:resourceType"
                                              sling:resourceType="granite/ui/components/coral/foundation/form/hidden"
                                              value="core/wcm/components/image/v2/image"/>
                           <imageTest3 allowUpload="{Boolean}false"
                                       autoStart="{Boolean}false"
                                       class="cq-droptarget"
                                       cq-msm-lockable="./imageTest3/file"
                                       fieldLabel="Image Test3"
                                       fileNameParameter="./imageTest3/fileName"
                                       fileReferenceParameter="./imageTest3/fileReference"
                                       jcr:primaryType="nt:unstructured"
                                       mimeTypes="[image/gif,image/jpeg,image/png,image/webp,image/tiff,image/svg+xml]"
                                       multiple="{Boolean}false"
                                       name="./imageTest3/file"
                                       sling:resourceType="cq/gui/components/authoring/dialog/fileupload"
                                       title="Drag to select image"
                                       uploadUrl="${suffix.path}"
                                       useHTML5="{Boolean}true"/>
                           <imageTest4ResType jcr:primaryType="nt:unstructured"
                                              name="./imageTest4/sling:resourceType"
                                              sling:resourceType="granite/ui/components/coral/foundation/form/hidden"
                                              value="core/wcm/components/image/v2/image"/>
                           <imageTest4 allowUpload="{Boolean}false"
                                       autoStart="{Boolean}false"
                                       class="cq-droptarget"
                                       cq-msm-lockable="./imageTest4/file"
                                       fieldLabel="Image Test4"
                                       fileNameParameter="./imageTest4/fileName"
                                       fileReferenceParameter="./imageTest4/fileReference"
                                       jcr:primaryType="nt:unstructured"
                                       mimeTypes="[image/gif,image/jpeg,image/png,image/webp,image/tiff,image/svg+xml]"
                                       multiple="{Boolean}false"
                                       name="./imageTest4/file"
                                       sling:resourceType="cq/gui/components/authoring/dialog/fileupload"
                                       title="Drag to select image"
                                       uploadUrl="${suffix.path}"
                                       useHTML5="{Boolean}true"/>
                           <imageTest5ResType jcr:primaryType="nt:unstructured"
                                              name="./imageTest5/sling:resourceType"
                                              sling:resourceType="granite/ui/components/coral/foundation/form/hidden"
                                              value="core/wcm/components/image/v2/image"/>
                           <imageTest5 allowUpload="{Boolean}false"
                                       autoStart="{Boolean}false"
                                       class="cq-droptarget"
                                       cq-msm-lockable="./imageTest5/file"
                                       fieldLabel="Image Test5"
                                       fileNameParameter="./imageTest5/fileName"
                                       fileReferenceParameter="./imageTest5/fileReference"
                                       jcr:primaryType="nt:unstructured"
                                       mimeTypes="[image/gif,image/jpeg,image/png,image/webp,image/tiff,image/svg+xml]"
                                       multiple="{Boolean}false"
                                       name="./imageTest5/file"
                                       sling:resourceType="cq/gui/components/authoring/dialog/fileupload"
                                       title="Drag to select image"
                                       uploadUrl="${suffix.path}"
                                       useHTML5="{Boolean}true"/>
                           <imageTest6ResType jcr:primaryType="nt:unstructured"
                                              name="./imageTest6/sling:resourceType"
                                              sling:resourceType="granite/ui/components/coral/foundation/form/hidden"
                                              value="core/wcm/components/image/v2/image"/>
                           <imageTest6 allowUpload="{Boolean}false"
                                       autoStart="{Boolean}false"
                                       class="cq-droptarget"
                                       cq-msm-lockable="./imageTest6/file"
                                       fieldLabel="Image Test6"
                                       fileNameParameter="./imageTest6/fileName"
                                       fileReferenceParameter="./imageTest6/fileReference"
                                       jcr:primaryType="nt:unstructured"
                                       mimeTypes="[image/gif,image/jpeg,image/png,image/webp,image/tiff,image/svg+xml]"
                                       multiple="{Boolean}false"
                                       name="./imageTest6/file"
                                       sling:resourceType="cq/gui/components/authoring/dialog/fileupload"
                                       title="Drag to select image"
                                       uploadUrl="${suffix.path}"
                                       useHTML5="{Boolean}true"/>
                        </items>
                     </column>
                  </items>
               </tab-2>
            </items>
         </tabs>
      </items>
   </content>
</jcr:root>
